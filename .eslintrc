{
  "extends": "eslint-config-airbnb",
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "padded-blocks": 0,
    "global-require": 0,
    "function-paren-newline": 0,
    "no-buffer-constructor": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-mutable-exports": 0,
    "react/jsx-filename-extension": 0
  }
}